{
  "manifest_version": 3,
  "name": "Ikariam 3Click",
  "version": "2.5",
  "permissions": [
    "webRequest",
    "activeTab",
    "storage"
  ],
  "host_permissions": [
    "https://*.ikariam.gameforge.com/*",
    "https://lobby.ikariam.gameforge.com/*"
  ],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "action": {
    "default_popup": "popup.html"
  }
}


