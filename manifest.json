{
  "manifest_version": 3,
  "name": "Ikariam 3Click",
  "version": "1.5",
  "permissions": [
    "webRequest",
    "activeTab",
    "storage"
  ],
  "host_permissions": [
    "https://*.ikariam.gameforge.com/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  }
}

